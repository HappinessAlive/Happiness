local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
	local Window = Rayfield:CreateWindow({
		Name = "Examination",
		Icon = 109763877151262,
		LoadingTitle = "Examination hub",
		LoadingSubtitle = "by mktlvu",
		ShowText = "Examination hub",
		Theme = "Ocean",
		ToggleUIKeybind = "K",
		DisableRayfieldPrompts = false,
		DisableBuildWarnings = false,
		ConfigurationSaving = {
    		Enabled = true,
    		FolderName = "Examination",
			FileName = "Hub"
		}
	})

	Rayfield:Notify({
		Title = "Thanks for using Examination hub",
		Content = "Message to developers: BRING FLARE BACK!!!1",
		Duration = 6.5,
		Image = 4483362458,
	})

	local Main = Window:CreateTab("Main", "crown")
		local Button = Main:CreateButton({
			Name = "Burn yourself",
			Callback = function()
				game.Players.LocalPlayer.Character.IsHunter.Value = true
				game.Players.LocalPlayer.Character.Humanoid.Health = 20
			end,
		})

		local Button = Main:CreateButton({
			Name = "Reset",
			Callback = function()
				game.Players.LocalPlayer.Character.Humanoid.Health = 0
			end,
		})

		local Toggle = Main:CreateToggle({
  			Name = "Third person",
			CurrentValue = false,
			Flag = "ThirdPersonToggle",
			Callback = function(v)
				if v == false then
					game.Players.LocalPlayer.CameraMode = Enum.CameraMode.LockFirstPerson
				end

				if v == true then
					game.Players.LocalPlayer.CameraMode = Enum.CameraMode.Classic
					game.Players.LocalPlayer.CameraMaxZoomDistance = 15
				end
			end,
		})

		local Toggle = Main:CreateToggle({
  			Name = "Chat history",
			CurrentValue = false,
			Flag = "ChatWindowToggle",
			Callback = function(v)
				if v == false then
					game.TextChatService.ChatWindowConfiguration.Enabled = false
				end

				if v == true then
					task.wait()
					game.TextChatService.ChatWindowConfiguration.Enabled = true
				end
			end,
		})

		local Slider = Main:CreateSlider({
			Name = "Increase maximum message bubble distance",
			Range = {0, 1000},
			Increment = 10,
			Suffix = "studs",
			CurrentValue = 100,
			Flag = "DistanceSlider",
			Callback = function(v)
				game.TextChatService.BubbleChatConfiguration.MaxDistance = v
			end,
		})

		local Divider = Main:CreateDivider()

		local Section = Main:CreateSection("Movement")
			local Slider = Main:CreateSlider({
				Name = "Walk speed",
				Range = {0, 500},
				Increment = 5,
				Suffix = "speed",
				CurrentValue = 7,
				Flag = "SpeedSlider",
				Callback = function(v)
					while true do
						task.wait()
						game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
					end
				end,
			})

			local Slider = Main:CreateSlider({
				Name = "Jump power",
				Range = {0, 100},
				Increment = 1,
				Suffix = "studs",
				CurrentValue = 2,
				Flag = "JumpSlider",
				Callback = function(v)
					game.Players.LocalPlayer.Character.Humanoid.UseJumpPower = false
					while true do
						task.wait()
						game.Players.LocalPlayer.Character.Humanoid.JumpHeight = v
					end
				end,
			})

	local Lighting = Window:CreateTab("Lighting", "zap")
		local Section = Lighting:CreateSection("Fog")
			local Button = Lighting:CreateButton({
				Name = "No fog",
				Callback = function()
					while true do
						task.wait()
						game.Lighting.FogStart = 1000
						game.Lighting.FogEnd = 1000
					end
				end,
			})

			local Slider = Lighting:CreateSlider({
				Name = "Fog start",
				Range = {0, 1000},
				Increment = 50,
				Suffix = "studs",
				CurrentValue = 0,
				Flag = "FogStartSlider",
				Callback = function(v)
					while true do
						task.wait()
						game.Lighting.FogStart = v
					end
				end,
			})

			local FogEndSlider = Lighting:CreateSlider({
				Name = "Fog end",
				Range = {0, 1000},
				Increment = 50,
				Suffix = "studs",
				CurrentValue = 0,
				Flag = "FogEndSlider",
				Callback = function(v)
					while true do
						task.wait()
						game.Lighting.FogEnd = v
					end
				end,
			})

		local Divider = Lighting:CreateDivider()

		local Section = Lighting:CreateSection("Brightness")
			local Toggle = Lighting:CreateToggle({
  				Name = "Fullbright",
				CurrentValue = false,
				Flag = "FullbrightToggle",
				Callback = function(v)
					if v == false then
						game.Lighting.Ambient = Color3.fromRGB(0,0,0)

					elseif v == true then
						game.Lighting.Ambient = Color3.fromRGB(255,255,255)
					end
				end,
			})

			local ColorPicker = Lighting:CreateColorPicker({
				Name = "Ambient color",
				Color = Color3.fromRGB(255,255,255),
				Flag = "AmbientColorPicker",
				Callback = function(v)
						wait(5)
						game.Lighting.Ambient = v
				end
			})

	local GUI = Window:CreateTab("GUI", "panel-left")
		local Section = GUI:CreateSection("Death UI")
			local Toggle = GUI:CreateToggle({
				Name = "Hide death UI",
				CurrentValue = false,
				Flag = "DeathUIToggle",
				Callback = function(v)
					if v == false then
						game.Players.LocalPlayer.PlayerGui.Death.Enabled = true
					elseif v == true then
						game.Players.LocalPlayer.PlayerGui.Death.Enabled = false
					end
				end,
			})

		local Divider = GUI:CreateDivider()

		local Section = GUI:CreateSection("Gasmask UI")
			local Toggle = GUI:CreateToggle({
				Name = "Hide gasmask UI",
				CurrentValue = false,
				Flag = "GasmaskUIToggle",
				Callback = function(v)
					if v == false then
						game.Players.LocalPlayer.PlayerGui.Gasmask.Enabled = true
					elseif v == true then
						game.Players.LocalPlayer.PlayerGui.Gasmask.Enabled = false
					end
				end,
			})

		local Divider = GUI:CreateDivider()

		local InfectionSection = GUI:CreateSection("Infection")
			local Label = GUI:CreateLabel("You must equip class for correct work of controllable infected UI", "circle-alert")

			local Animation = Instance.new("Animation")
			Animation.AnimationId = "rbxassetid://136775254837264"
			local Sound = game.Players.LocalPlayer.PlayerGui.Gasmask.MaskHole.Infection.ACTIVATE
			local Music = game.Players.LocalPlayer.PlayerGui.Gasmask.MaskHole.Infection.HATRED
			local Red = Instance.new("ColorCorrectionEffect")
			Red.Name = "InfectionColorCorrection"
			Red.Parent = game.Lighting
			Red.Enabled = false
			Red.TintColor = Color3.fromRGB(255,0,0)

			local Toggle = GUI:CreateToggle({
				Name = "Enable controllable infected UI",
				CurrentValue = false,
				Flag = "InfectedUIToggle",
				Callback = function(v)
					local AnimationTrack = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Animation)
					if v == false then
						Red.Enabled = false
						Music.Playing = false
						game.Players.LocalPlayer.Character.Head.InfectedLeft.ParticleEmitter.Enabled = false
						game.Players.LocalPlayer.Character.Head.InfectedRight.ParticleEmitter.Enabled = false
						if game.Players.LocalPlayer.Character.Hat_Gear.Handle.Lens then
							game.Players.LocalPlayer.Character.Hat_Gear.Handle.Lens.Transparency = 0.05
						end
					elseif v == true then
						AnimationTrack:Play()
						Sound.Playing = true
						Music.Playing = true
						Red.Enabled = true
						game.Players.LocalPlayer.Character.Head.InfectedLeft.ParticleEmitter.Enabled = true
						game.Players.LocalPlayer.Character.Head.InfectedRight.ParticleEmitter.Enabled = true
						if game.Players.LocalPlayer.Character.Hat_Gear.Handle.Lens then
							game.Players.LocalPlayer.Character.Hat_Gear.Handle.Lens.Transparency = 0
						end
					end
				end,
			})

			local Divider = GUI:CreateDivider()

			local Toggle = GUI:CreateToggle({
				Name = "Enable ESP",
				CurrentValue = false,
				Flag = "ESPToggle",
				Callback = function(v)
					local Players = game:GetService("Players")
					local CharactersContainer = workspace:WaitForChild("Characters")
					local MenloColor = Color3.fromRGB(80, 109, 84)
					local HostileColor = Color3.fromRGB(255, 0, 0)
					local DefaultColor = Color3.fromRGB(255, 255, 255)

					local function GetCharacterTeam(Character)
						local Player = Players:GetPlayerFromCharacter(Character)
						if Player then
							return "Menlo"
						end

						local TeamValue = Character:FindFirstChild("Team", true) 
						if TeamValue and TeamValue:IsA("StringValue") then
							local TeamName = TeamValue.Value
							if TeamName == "RIF" then
								return "RIF"
							end
						end
					return nil
					end

					local function CreateHighlightForCharacter(Character)
						local Highlight = Character:FindFirstChildOfClass("Highlight")
						if not Highlight then
							Highlight = Instance.new("Highlight")
							Highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
							Highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
							Highlight.OutlineTransparency = 0
							Highlight.Enabled = false
							Highlight.Parent = Character
						end

						local Team = GetCharacterTeam(Character)
						local FillColor = HostileColor
						if Team == "Menlo" then
							FillColor = MenloColor
						elseif team == "RIF" then
							FillColor = HostileColor
						end

						Highlight.FillColor = FillColor
					end

					for _, Character in ipairs(CharactersContainer:GetChildren()) do
						CreateHighlightForCharacter(Character)
					end

					CharactersContainer.ChildAdded:Connect(CreateHighlightForCharacter)

					if v == false then
						local function DisableAllHighlights()
   							for _, Character in ipairs(CharactersContainer:GetChildren()) do
       							local Highlight = Character:FindFirstChildOfClass("Highlight")
      							if Highlight then
        							Highlight.Enabled = false
        						end
    						end
						end

						DisableAllHighlights()

					elseif v == true then
						local function EnableAllHighlights()
   							for _, Character in ipairs(CharactersContainer:GetChildren()) do
     							local Highlight = Character:FindFirstChildOfClass("Highlight")
    							if Highlight then
        							Highlight.Enabled = true
        						end
    						end
						end

						EnableAllHighlights()
					end
				end,
			})

