local GUITab = Window:CreateTab("GUI", "panel-left")

local NoDeathSection = GUITab:CreateSection("Death UI")
local NoDeathButton = GUITab:CreateButton({
	Name = "Hide death UI",
	Callback = function()
		local LocalPlayer = game.Players.LocalPlayer
		LocalPlayer.PlayerGui.Death.Enabled = false

		local function onCharacterAdded(Character)
			local Humanoid = Character:WaitForChild("Humanoid")

			Humanoid.Died:Connect(function()
				LocalPlayer.PlayerGui.Death.Enabled = false
			end)
		end

		if Character then
			onCharacterAdded(LocalPlayer.Character)
		end

		LocalPlayer.CharacterAdded:Connect(onCharacterAdded)
	end,
})

local NoDeathKeybind = GUITab:CreateKeybind({
	Name = "Hide death UI (keybind)",
	CurrentKeybind = "M",
	HoldToInteract = false,
	Flag = "NoDeathKeybind",
	Callback = function()
		local LocalPlayer = game.Players.LocalPlayer
		LocalPlayer.PlayerGui.Death.Enabled = false

		local function onCharacterAdded(Character)
			local Humanoid = Character:WaitForChild("Humanoid")

			Humanoid.Died:Connect(function()
				LocalPlayer.PlayerGui.Death.Enabled = false
			end)
		end

		if Character then
			onCharacterAdded(LocalPlayer.Character)
		end

		LocalPlayer.CharacterAdded:Connect(onCharacterAdded)
	end,
})

local Divider = GUITab:CreateDivider()

local GasmaskSection = GUITab:CreateSection("Gasmask")
local NoGasmaskButton = GUITab:CreateButton({
