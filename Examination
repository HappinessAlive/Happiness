local ChatSection = MainTab:CreateSection("Chat")

local WindowButton = MainTab:CreateButton({
	Name = "Enable chat window",
	Callback = function()
		while true do
			game.TextChatService.ChatWindowConfiguration.Enabled = true
			task.wait(5)
		end
	end,
})

