loadstring(game:HttpGet('https://raw.githubusercontent.com/DarkNetworks/Infinite-Yield/main/latest.lua'))()

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
game.Players.LocalPlayer.PlayerGui.MouseUnlock.Enabled = true

local Window = Rayfield:CreateWindow({
	Name = "Examination",
	Icon = 109763877151262,
	LoadingTitle = "Examination hub",
	LoadingSubtitle = "by mktlvu",
	ShowText = "Examination hub",
	Theme = "Default",
	ToggleUIKeybind = "K",
	DisableRayfieldPrompts = false,
	DisableBuildWarnings = false,
	ConfigurationSaving = {
    	Enabled = true,
    	FolderName = "Examination",
		FileName = "Examination hub"
	}
})

Rayfield:Notify({
	Title = "Thanks for using Examination hub",
	Content = "Message to developers: BRING FLARE BACK!!!1",
	Duration = 6.5,
	Image = 4483362458,
})

local MainTab = Window:CreateTab("Main", "crown")

local BurnSection = MainTab:CreateSection("Burnout")
local BurnButton = MainTab:CreateButton({
	Name = "Burn yourself",
	Callback = function()
		game.Players.LocalPlayer.Character.IsHunter.Value = true
		game.Players.LocalPlayer.Character.Humanoid.Health = 20
	end,
})

local BurnKeybind = MainTab:CreateKeybind({
	Name = "Burn keybind",
	CurrentKeybind = "B",
	HoldToInteract = false,
	Flag = "BurnKeybind",
	Callback = function()
		game.Players.LocalPlayer.Character.IsHunter.Value = true
		game.Players.LocalPlayer.Character.Humanoid.Health = 20
	end,
})

local Divider = MainTab:CreateDivider()

local ResetSection = MainTab:CreateSection("Reset")
local ResetButton = MainTab:CreateButton({
	Name = "Reset",
	Callback = function()
		game.Players.LocalPlayer.Character.Humanoid.Health = 0
	end,
})

local ResetKeybind = MainTab:CreateKeybind({
	Name = "Reset keybind",
	CurrentKeybind = "V",
	HoldToInteract = false,
	Flag = "Keybind2",
	Callback = function()
		game.Players.LocalPlayer.Character.Humanoid.Health = 0
