			local Toggle = Fun:CreateToggle({
				Name = "Chimera's execution animation",
				CurrentValue = false,
				Flag = "ExecutionAnimation",
				Callback = function(v)
					local Animator = game.Players.LocalPlayer.Character.Humanoid.Animator
					local AnimationTrack
					local Condition = v
					local function PlayOrStopAnimation(Condition)
						if Condition then
        					if not AnimationTrack then
            					local Animation = Instance.new("Animation")
        						Animation.AnimationId = "rbxassetid://97305733594978"
								ExecutionTrack = Animator:LoadAnimation(Animation)
								ExecutionTrack.Looped = true
    						end

    						ExecutionTrack:Play()

						else
   							if ExecutionTrack then
								ExecutionTrack:Stop()
        						ExecutionTrack:Destroy()
    						end
						end
					end
					PlayOrStopAnimation(Condition)
				end,
			})

