	Name = "JumpPower",
	Range = {0, 100},
	Increment = 1,
	Suffix = "studs",
	CurrentValue = 2,
	Flag = "JumpSlider",
	Callback = function(v)
		game.Players.LocalPlayer.Character.Humanoid.UseJumpPower = false

		while true do
			task.wait()
			game.Players.LocalPlayer.Character.Humanoid.JumpHeight = v
		end
	end,
})

